<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>Yugen.Toolkit.Uwp.Controls</id>
    <version>1.0.40</version>
    <title>Yugen Toolkit Uwp Controls</title>
    <authors>emiliano84</authors>
    <owners>emiliano84</owners>
    <license type="expression">GPL-3.0-or-later</license>
    <projectUrl>https://github.com/emiliano84/Yugen.Toolkit</projectUrl>
    <icon>images\icon.png</icon>
    <description>
      This library provides XAML templated controls. It is part of the Yugen Toolkit.

      Controls:
        - Dialogs
        - Graphs
        - Menu
        - Validation
    </description>
    <releaseNotes></releaseNotes>
    <copyright></copyright>
    <tags>Yugen Toolkit Uwp Controls XAML Dialogs Graphs Menu Validation</tags>
    <repository type="git" url="https://github.com/emiliano84/Yugen.Toolkit.git" /> 
    <dependencies>
      <group targetFramework="uap10.0.17763">
        <dependency id="Yugen.Toolkit.Standard" version="1.0.40" exclude="Build,Analyzers" />
        <dependency id="Yugen.Toolkit.Uwp" version="1.0.40" exclude="Build,Analyzers" />
        <dependency id="Microsoft.NETCore.UniversalWindowsPlatform" version="6.2.10" exclude="Build,Analyzers" />
      </group>
    </dependencies>
  </metadata>
  <files>  
    <!-- Dll -->
    <file src="bin\Release\Yugen.Toolkit.Uwp.Controls.dll" target="lib\uap10.0.17763" /> 

    <!-- Resources -->
    <file src="bin\Release\Yugen.Toolkit.Uwp.Controls.pdb" target="lib\uap10.0.17763" />
    <file src="bin\Release\Yugen.Toolkit.Uwp.Controls.pri" target="lib\uap10.0.17763" />  

    <!-- IntelliSense -->
    <file src="bin\Release\Yugen.Toolkit.Uwp.Controls.XML" target="lib\uap10.0.17763" />

    <!-- XAML control -->
    <file src="bin\Release\Yugen.Toolkit.Uwp.Controls\**\*.*" target="lib\uap10.0.17763\Yugen.Toolkit.Uwp.Controls" />

    <!-- Icon -->
    <file src="..\icon.png" target="images\" />
  </files>
</package>